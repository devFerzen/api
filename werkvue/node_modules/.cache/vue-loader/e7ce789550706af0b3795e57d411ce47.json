l\">\r\n            <b-button v-on:click=\"activeRol('freelancer')\" v-bind:class=\"[usuarioRegistro.rol == 'freelancer' ? 'btn-rol-selected' : '' ]\" class=\"btn-rol werk-shadow \">FREELANCER</b-button>\r\n          </b-col>\r\n          <b-col class=\"col-rol\">\r\n            <b-button v-on:click=\"activeRol('contratante')\" v-bind:class=\"[usuarioRegistro.rol == 'contratante' ? 'btn-rol-selected' : '' ]\" class=\"btn-rol werk-shadow \">CONTRATANTE</b-button>\r\n          </b-col>\r\n        </b-row>\r\n      </div>\r\n\r\n      <b-form-group id=\"CorreoLableRPModal\" label=\"E-MAIL\" label-for=\"CorreoInputRPModal\" class=\"lable-RP-Modal first-form-group\">\r\n        <b-form-input id=\"CorreoInputRPModal\" v-model=\"usuarioRegistro.correo\" class=\"input-RP-Modal werk-shadow\" placeholder=\"Teclea aquí\" required>\r\n        </b-form-input>\r\n      </b-form-group>\r\n    </b-form><!--form-modal-->\r\n    <div class=\"acciones-modal\">\r\n      <b-button pill class=\"btn-continuar shadow-none\">CONTINUAR</b-button>\r\n\r\n      <b-button class=\"btn-redes-sociales btn-facebook shadow-none\">\r\n        <font-awesome-icon class=\"btn-redes-sociales-icon\" style=\"color:white;\" size=\"2x\" :icon=\"['fab', 'facebook-square']\"  />\r\n        <p>Registrarse con Facebook</p>\r\n      </b-button>\r\n      <b-button class=\"btn-redes-sociales werk-shadow\">\r\n        <font-awesome-icon class=\"btn-redes-sociales-icon\" size=\"2x\" :icon=\"['fab', 'google']\" />\r\n        <p>Registrarse con Google</p>\r\n      </b-button>\r\n      <b-button class=\"btn-redes-sociales werk-shadow\">\r\n        <font-awesome-icon class=\"btn-redes-sociales-icon\" size=\"2x\" :icon=\"['fab', 'apple']\" />\r\n        <p>Registrarse con Apple</p>\r\n      </b-button>\r\n      <p>Al unirte, aceptas recibir e-mails de <b>werk..</b></p>\r\n    </div><!--acciones-modal-->\r\n  </b-modal>\r\n</template>\r\n<style>\r\n  .modal-sm {\r\n    max-width:389px!important;\r\n    width:389px!important;\r\n  }\r\n</style>\r\n<style scoped>\r\n\r\n  .modal-content {\r\n    -webkit-border-radius: 50px!important;\r\n    -moz-border-radius: 50px!important;\r\n    border-radius: 50px!important;\r\n  }\r\n\r\n  .col-rol {\r\n    display: flex;\r\n    justify-content: center;\r\n  }\r\n\r\n  .registro-roles {\r\n    width: 315px;\r\n    margin: auto;\r\n    margin-top: 45px;\r\n  }\r\n\r\n  .werk-label {\r\n    font-weight: 700;\r\n    font-size: 14px;\r\n    margin-left: 37px;\r\n  }\r\n\r\n  .btn-rol{\r\n    width: 120px;\r\n    font-weight: 500;\r\n    font-size: 13px;\r\n    border-radius: 10px;\r\n    height: 38px;\r\n    border: none;\r\n    background-color: white;\r\n    color: #6a6a6a;\r\n    font-weight: 500;\r\n\r\n  }\r\n\r\n  .btn-rol:hover{\r\n    color: #519482!important;\r\n    font-weight: 700;\r\n  }\r\n\r\n  .btn-rol-selected{\r\n    background-color: #9fe1b3!important;\r\n    color: #519482!important;\r\n    font-weight: 800!important;\r\n  }\r\n\r\n  .btn-continuar {\r\n    width:296px;\r\n    height: 44px;\r\n    background-color: #7ed89a;\r\n    border: 0 solid transparent;\r\n    margin-top:23px;\r\n    color: #15365f;\r\n    font-size: 19px;\r\n    font-weight: 800;\r\n  }\r\n\r\n  .btn-continuar:active {\r\n    background-color: #9fe1b3!important;\r\n    color: #15365f!important;\r\n  }\r\n\r\n  .btn-redes-sociales:hover {\r\n    background-color: #7ed89a!important;\r\n    color: white;\r\n  }\r\n\r\n  .btn-redes-sociales {\r\n    width: 266px;\r\n    height: 45px;\r\n    margin-top: 19px;\r\n    border-radius: 10px;\r\n    background-color: white;\r\n    color: black;\r\n    font-weight: 500;\r\n    border: 0 solid transparent;\r\n    font-size: 13px;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n  }\r\n\r\n  .btn-redes-sociales-icon{\r\n    margin-right: 15px;\r\n  }\r\n\r\n  .btn-redes-sociales p {\r\n    width: auto!important;\r\n  }\r\n\r\n  .btn-facebook{\r\n    background-color: #3f5793!important;\r\n    color:white;\r\n  }\r\n\r\n  .first-form-group{\r\n      margin-top: 19px;\r\n      margin-bottom: 23px;\r\n  }\r\n\r\n  .form-group{\r\n    margin-right: auto;\r\n    margin-left: auto;\r\n  }\r\n  .lable-RP-Modal label{\r\n    padding-left: 12px;\r\n  }\r\n\r\n  .lable-RP-Modal{\r\n    font-weight: bold;\r\n    font-size: 14px;\r\n    width:266px;\r\n  }\r\n\r\n  .input-RP-Modal{\r\n    height: 45px;\r\n    font-size: 13px;\r\n    border-radius: 10px;\r\n    width:266px;\r\n  }\r\n\r\n  .acciones-modal{\r\n    text-align: center;\r\n    margin-top: 23px;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n    flex-direction: column;\r\n  }\r\n\r\n  .acciones-modal p{\r\n    font-size: 11px;\r\n    width: 230px;\r\n    margin-top: 30px;\r\n    margin-bottom: 30px;\r\n  }\r\n\r\n</style>\r\n\r\n<script>\r\n/*\r\n* No se hará un modal global dentro de VUEX porque no sabe\r\n* si cambiaremos a un state management de GRAPHQL\r\n*/\r\n\r\nimport ISModalBus from '../../plugins/InicioSesionPlugin.js';\r\nimport { gql } from \"apollo-boost\";\r\nimport { mapActions } from 'vuex'\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      title: '',\r\n      text: '',\r\n      // adding callback function variable\r\n      onRegistro: {},\r\n      usuarioRegistro: {\r\n        rol: 'freelancer',\r\n        correo: 'pruebaSimulacroDOS@gmail.com',\r\n        password: '123456789',\r\n        repPassword: '123456789',\r\n        nombre: 'Sr. Prueba Simulacro DOS'\r\n      },\r\n      creandoUsuarioGql: gql `\r\n        mutation CREANDO_USUARIO(\r\n          $rol: String!\r\n          $nombre: String!\r\n          $correo: String!\r\n          $password: String!\r\n        ){\r\n        creandoUsuario(input:{\r\n          rol: $rol\r\n          nombre: $nombre\r\n          correo: $correo\r\n          password: $password\r\n        }){\r\n          _id\r\n          nombre\r\n          rol\r\n          }\r\n        }\r\n      `,\r\n    }\r\n  },\r\n  methods: {\r\n    ...mapActions({\r\n      actUserStore: 'Autenticacion/actUserStore'\r\n    }),\r\n    registroUserStore(val) {\r\n      //Verificar si apolloquery es un async funciton\r\n      console.log(\"usuario que se registrará es \", val);\r\n      this.actUserStore(val);\r\n    },\r\n    RHideModal() {\r\n      // this method is unchanged\r\n      this.$bvModal.hide('RegistroPrincipalModalID');\r\n    },\r\n    registroUserStore(val) {\r\n      // we must check if this.onRegistro is function\r\n      if(typeof this.onRegistro === 'function') {\r\n        // run passed function and then close the modal\r\n        this.actUserStore(val);\r\n        this.onRegistro();\r\n        this.RHideModal();\r\n      } else {\r\n        // we only close the modal\r\n        this.RHideModal();\r\n      }\r\n    },\r\n    showRegistrar(params) {\r\n      // making modal visible\r\n      this.$bvModal.show('RegistroPrincipalModalID');\r\n      // setting title and text\r\n      this.title = params.title;\r\n      this.text = params.text;\r\n      // setting callback function\r\n      this.onRegistro = params.onRegistro;\r\n    },\r\n    activeRol(activeRol){\r\n      this.usuarioRegistro.rol = activeRol;\r\n\r\n      //Add and remove class\r\n    }\r\n  },\r\n  beforeMount() {\r\n    // here we need to listen for emited events\r\n    // we declared those events inside our plugin\r\n    ISModalBus.EventBus.$on('showRegistrar', (params) => {\r\n      this.showRegistrar(params)\r\n    })\r\n  }\r\n}\r\n\r\n</script>\r\n"]}]}