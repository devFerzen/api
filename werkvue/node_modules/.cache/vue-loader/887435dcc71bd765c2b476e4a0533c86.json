red: \"faTwitter\",\r\n            url: \"url@urltwitter\"\r\n          },\r\n          {\r\n            red: \"faLinkedin\",\r\n            url: \"url@urllinkedin\"\r\n          }]\r\n        },\r\n        werker: {\r\n          nombre: {\r\n            nombres: \"Puto el que\",\r\n            apellidos: \"Lolea Utop\"\r\n          },\r\n          ubicacion: {\r\n            pais: \"MX\",\r\n            estado: \"NLE\",\r\n            ciudad: \"MTY\"\r\n          },\r\n        },\r\n        objeto_werk: {\r\n          tipo: 'vacante',\r\n          esquemas: [1,2,3],\r\n          capacidad: [1,2],\r\n          estatus: {\r\n            tipo: true // AFSS - este debe que ser marcado ya sea en el validador del front o back\r\n          }\r\n        }\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    vacanteSaveUpdate(){\r\n      let params = {};\r\n      params.input = this.VacanteInfo;\r\n\r\n      if (this.id !== '0') {\r\n        params.id = this.id;\r\n      }\r\n\r\n      const mutateAnswer = this.$apollo.mutate({\r\n        mutation: this.id === '0' ? WERKOBJECT_NEW : WERKOBJECT_UPDATE,\r\n        variable: {\r\n          params\r\n        }\r\n      });\r\n    },\r\n    getVancanteInfo: async(params) => {\r\n      if (params.id === '0'){\r\n        return;\r\n      }\r\n\r\n      const getQueryResult = await this.$apollo.query({\r\n        query: WERK_OBJECT_QUERY,\r\n        variables: {\r\n          params\r\n        }\r\n      });\r\n      console.dir(getQueryResult);\r\n      this.VacanteInfo = getQueryResult;\r\n    }\r\n  },\r\n  async created(){\r\n    let params = {};\r\n    params.id = this.id;\r\n\r\n    await this.getVancanteInfo(params);\r\n  },\r\n  watch: {\r\n    //Aquí estará el watch para mandar a cambiar la información del componente\r\n    //ya que no se renderiza el componente ni vuelve a pasar por los hook cycles\r\n    //así que se hablará el wath para checar su url.\r\n    $route(to, from){\r\n      //Reaccionar a los cambios\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n  .informacion-general-vacante, .rangoSalarial-tags,\r\n  .informacion-personal-contratante, .acciones-edit-vacante {\r\n    width: 100%;\r\n  }\r\n\r\n  .informacion-personal-contratante {\r\n    height: auto;\r\n  }\r\n\r\n  .informacion-personal-contratante-title {\r\n    font-weight:900;\r\n    color:black;\r\n    margin-bottom: 8px;\r\n    margin-left: 0;\r\n    text-align: center;\r\n  }\r\n\r\n  .info-personal-contratante-body {\r\n    width: 60%;\r\n    margin-left: auto;\r\n    margin-right: auto;\r\n  }\r\n\r\n  .informacion-personal-contratante .tw-contactame-titulo {\r\n    font-family: 'MadeTommyExtraBold';\r\n  }\r\n\r\n  .informacion-personal-contratante p{\r\n    margin-bottom: 0;\r\n    margin-left: 6px;\r\n  }\r\n\r\n  .informacion-personal-contratante span{\r\n    font-family: 'MadeTommyReg';\r\n    font-size: 12px;\r\n    margin-left: 6px;\r\n  }\r\n\r\n  .informacion-personal-contratante .tw-redes {\r\n    margin-top: 5px;\r\n  }\r\n\r\n  .informacion-personal-contratante .tw-redes-box {\r\n    width: 30px;\r\n    height: 30px;\r\n    border-radius: 5px;\r\n    background-color: #8EE4AF;\r\n    text-align: center;\r\n    -webkit-writing-mode: vertical-rl;\r\n    writing-mode: vertical-rl;\r\n    cursor: pointer;\r\n    margin-left: auto;\r\n    margin-right: auto;\r\n  }\r\n\r\n  .informacion-personal-contratante .tw-redes-icons {\r\n    color: white;\r\n    font-size: 18px;\r\n  }\r\n\r\n  .informacion-personal-contratante .tw-contactame-icons {\r\n    font-size: 19px;\r\n    margin-right: 5px;\r\n  }\r\n\r\n  .informacion-personal-contratante .tw-contactame-texto {\r\n    font-size: .8em;\r\n    margin-bottom: 0;\r\n  }\r\n\r\n  @media only screen and (min-width:992px) {\r\n    .informacion-general-vacante, .rangoSalarial-tags,\r\n    .informacion-personal-contratante, .acciones-edit-vacante {\r\n      width: 50%;\r\n    }\r\n\r\n    .info-personal-contratante-body {\r\n      margin-left: 0;\r\n      margin-right: 0;\r\n    }\r\n\r\n    .informacion-personal-contratante-title {\r\n        text-align: left;\r\n      }\r\n  }\r\n\r\n  .vacante-edit-seccion {\r\n    padding-left: 18px;\r\n    padding-right: 18px;\r\n    margin-top: 1.2rem;\r\n  }\r\n\r\n  .acciones-vacante-edit-body{\r\n    align-self: flex-end;\r\n    margin-left: auto;\r\n    margin-right: auto;\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n  }\r\n\r\n  .acciones-vacante-edit-body .btn-borrador {\r\n    font-size: 12px !important;\r\n    width: 185px !important;\r\n    height: 28px !important;\r\n    border: 0px !important;\r\n    background-color: #DBDADA;\r\n    color: #15365f!important;\r\n    font-weight: 600;\r\n  }\r\n\r\n  .acciones-vacante-edit-body .btn-publicar {\r\n    font-size: 15px !important;\r\n    width: 245px !important;\r\n    height: 33px !important;\r\n    border: 0px !important;\r\n    background-color: #5CDB95;\r\n    color: #15365f!important;\r\n    font-weight: 600;\r\n  }\r\n\r\n  .b-form-tags {\r\n      background-color: inherit;\r\n      border: none;\r\n      padding-left: 0;\r\n      padding-right: 0;\r\n  }\r\n\r\n  .badge-tags {\r\n    font-size: 0.7em!important;\r\n    line-height: 1em;\r\n    padding: 5px 15px;\r\n    border-radius: 20px;\r\n    color: #379683;\r\n    background-color: #8EE4AF;\r\n  }\r\n\r\n  .badge-tags:hover {\r\n    color: #379683;\r\n    background-color: #5CD895;\r\n  }\r\n</style>\r\n"]}]}